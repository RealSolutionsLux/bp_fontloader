		<!-- Add Themes using fontloader -->
		<template id="website.theme_amelia" name="Amelia" inherit_id="website.theme" active="False" customize_show="True">
			<xpath expr="//link[@id='bootstrap_css']" position="replace">
				<t t-if="website and website.use_fontloader">
					<link rel='stylesheet' href='/bp_fontloader/static/src/css/bootswatch/amelia.min.css' t-ignore="true" />
					<link rel='stylesheet' href='/website/static/src/css/bootswatch/amelia.fix.css' t-ignore="true" />
					<script type="text/javascript" src="/bp_fontloader/static/src/js/bootswatch/amelia.js" />
					<script type="text/javascript" src="/bp_fontloader/static/src/js/fontloader.js" />
				</t>
				<t t-if="not website or not website.use_fontloader">
					<link rel='stylesheet' href='/website/static/src/css/bootswatch/amelia.min.css' t-ignore="true" />
					<link rel='stylesheet' href='/website/static/src/css/bootswatch/amelia.fix.css' t-ignore="true" />
				</t>
			</xpath>
		</template>

